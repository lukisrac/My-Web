{
  "name": "mujweb",
  "version": "1.1.0",
  "description": "Můj osobní web, předělaný pomocí Parcel JS",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html src/contact.php --open",
    "prebuild": "rimraf dist",
    "build": "parcel build src/index.html --out-file index.php src/contact.php --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klobasus/My-Web.git"
  },
  "keywords": [
    "můj",
    "web",
    "osobní"
  ],
  "author": "Lukáš Rác",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/klobasus/My-Web/issues"
  },
  "homepage": "https://github.com/klobasus/My-Web#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.2",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "sass": "^1.26.3"
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      ".js.map": "js",
      ".css": "css",
      ".css.map": "css",
      "images": [
        ".jpg",
        ".png",
        ".ico"
      ],
      ".fileExtension": "folder/in/dist",
      "folder/in/dist": [
        ".file",
        ".extensions"
      ]
    }
  },
  "dependencies": {
    "firebase": "^7.13.1",
    "isotope-layout": "^3.0.6",
    "smooth-scroll": "^16.1.3"
  }
}
